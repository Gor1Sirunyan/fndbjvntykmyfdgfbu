<% extend('layout') %>
<div id="container">

    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>University</th>

        </tr>
        </thead>
        <div>
            <tbody>
            <% universities.forEach(function(row){ %>
                <tr>
                    <td><%= row.universitiesID %></td>
                    <td><%= row.place_of_study %></td>
                </tr>
            <% }) %>


            </tbody>

        </div >

    </table>
</div>
